  "name": "ft_transcendence",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "front": "npx tsc --project tsconfig.front.json",
    "back": "npx tsc --project tsconfig.back.json",
    "start": "npm run front && npm run back && node ./back/server/server.js",
    "dev": "npm run front && npm run back && npx ts-node-dev --respawn --transpile-only ./back/server/server.ts",
    "watch:front": "npx tsc --project tsconfig.front.json --watch",
    "watch:back": "npx tsc --project tsconfig.back.json --watch",
    "dev:full": "concurrently \"npm run watch:front\" \"npm run watch:back\" \"npx ts-node-dev --respawn --transpile-only ./back/server/server.ts\""
  },
